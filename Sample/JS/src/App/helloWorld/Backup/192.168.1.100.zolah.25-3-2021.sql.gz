# ------------------------------------------------------------
# SCHEMA DUMP FOR TABLE: zolah_berita_detail
# ------------------------------------------------------------

CREATE TABLE IF NOT EXISTS `zolah_berita_detail` (
  `id_berita` bigint(20) NOT NULL AUTO_INCREMENT,
  `judul_berita` text NOT NULL,
  `seo_url` text NOT NULL,
  `id_kategori` bigint(20) NOT NULL,
  `isi_berita` text NOT NULL,
  `gambar` text NOT NULL,
  `id_user` bigint(20) NOT NULL,
  `waktu_berita` timestamp NOT NULL DEFAULT current_timestamp(),
  `status` enum('true', 'false') NOT NULL,
  PRIMARY KEY (`id_berita`),
  KEY `id_kategori` (`id_kategori`)
) ENGINE = InnoDB AUTO_INCREMENT = 12 DEFAULT CHARSET = utf8mb4;

# ------------------------------------------------------------
# SCHEMA DUMP FOR TABLE: zolah_kategori
# ------------------------------------------------------------

CREATE TABLE IF NOT EXISTS `zolah_kategori` (
  `id_kategori` bigint(11) NOT NULL AUTO_INCREMENT,
  `nama_kategori` text NOT NULL,
  PRIMARY KEY (`id_kategori`)
) ENGINE = InnoDB AUTO_INCREMENT = 5 DEFAULT CHARSET = utf8mb4;

# ------------------------------------------------------------
# SCHEMA DUMP FOR TABLE: zolah_user_login
# ------------------------------------------------------------

CREATE TABLE IF NOT EXISTS `zolah_user_login` (
  `id_user` bigint(20) NOT NULL AUTO_INCREMENT,
  `nama_lengkap` text NOT NULL,
  `username` longtext NOT NULL,
  `password` longtext NOT NULL,
  `level_access` enum('admin', 'user') NOT NULL,
  `waktu` timestamp NOT NULL DEFAULT current_timestamp(),
  PRIMARY KEY (`id_user`)
) ENGINE = InnoDB AUTO_INCREMENT = 2 DEFAULT CHARSET = utf8mb4;

